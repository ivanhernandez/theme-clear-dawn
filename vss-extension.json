{
    "manifestVersion": 1,
    "id": "theme-clear-dawn",
    "version": "0.6.0",
    "name": "Clear Dawn",
    "publisher": "ivanhernandez",
    "description": "A desaturated, high contrast, light theme (and dark variant)",
    "license": "SEE LICENSE IN License.md",
    "engines": {
        "vscode": "^1.13.0"
    },
    "targets": [{
        "id": "Microsoft.VisualStudio.Services"
    }],
    "icons": {
        "default": "logo.png"
    },
    "categories": [
        "Themes"
    ],
