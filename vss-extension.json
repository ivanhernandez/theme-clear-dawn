    },
    "content": {
        "details": {
            "path": "readme.md"
        },
        "license": {
            "path": "LICENSE.txt"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/ivanhernandez/theme-clear-dawn"
    }
