{
    "$schema": "vscode://schemas/color-theme",
    "name": "Clear Dawn",
    "colors": {
        "editor.lineHighlightBackground": "#FFFDFA",
        "editor.lineHighlightBorder": "#FFF",

        "activityBar.background": "#414861", //rgb(65, 72, 97)
        "activityBarBadge.background": "#614151", //rgb(97, 65, 81)
        "badge.background": "#715141", //rgb(113, 81, 65)
        "badge.foreground": "#FEF6F0", //rgb(254, 246, 240)
        "button.background": "#715141", //rgb(113, 81, 65)
        "button.foreground": "#FEF6F0", //rgb(254, 246, 240)
        "button.hoverBackground": "#D08030", //rgb(208, 128, 48)
        "descriptionForeground": "#D08030", //rgb(208, 128, 48)
        "foreground": "#715141", //rgb(113, 81, 65)

        "editor.background": "#FAF4F0", //rgb(250, 244, 240)
        "editor.foreground": "#100800", //rgb(16, 8, 0)
        "editor.selectionBackground": "#BDC7E3", //rgb(189, 199, 227)
        "editor.inactiveSelectionBackground": "#EBF0F8", //rgb(235, 240, 248)
        "editor.selectionHighlightBackground": "#EBF0F8", //rgb(235, 240, 248)
        "editorCursor.foreground": "#D08030", //rgb(208, 128, 48)
        "editorIndentGuide.background": "#D3D3D3", //rgb(211, 211, 211)
        "editorSuggestWidget.background": "#F3F3F3", //rgb(243, 243, 243)

        "input.background": "#FFFFFF", //rgb(255, 255, 255)
        "input.foreground": "#715141", //rgb(113, 81, 65)
        "list.highlightForeground": "#FF6060", //rgb(255, 96, 96)
        "list.focusBackground": "#FFD0D0", //rgb(255, 208, 208)
        "list.hoverBackground": "#FFF0F0", //rgb(255, 240, 240)
        "panel.background": "#e9d7d1", //WAS rgb(232, 208, 200)
        "panel.border": "#715141", //rgb(113, 81, 65)
        "sideBar.background": "#e9d7d1", //WAS rgb(232, 208, 200)
        "sideBar.foreground": "#100800", //rgb(16, 8, 0)
        "sideBar.border": "#715141", //rgb(113, 81, 65)
        "sideBarTitle.foreground": "#6F6F6F", //rgb(111, 111, 111)
        "statusBar.background": "#414861", //rgb(65, 72, 97)
        "statusBar.debuggingBackground": "#813131", //rgb(129, 49, 49)
        "statusBar.foreground": "#FFFDF8", //rgb(255, 253, 248)
        "statusBar.noFolderBackground": "#614151", //rgb(97, 65, 81)

        "terminal.background": "#040800", //rgb(4, 8, 0)
        "terminal.foreground": "#C8E0C0", //rgb(200, 224, 192)
        "terminal.ansiBlack": "#040800", //rgb(4, 8, 0)
        "terminal.ansiBlue": "#004080", //rgb(0, 64, 128)
        "terminal.ansiCyan": "#009080", //rgb(0, 144, 128)
        "terminal.ansiGreen": "#209000", //rgb(32, 144, 0)
        "terminal.ansiMagenta": "#804080", //rgb(128, 64, 128)
        "terminal.ansiRed": "#804000", //rgb(128, 64, 0)
        "terminal.ansiYellow": "#809000", //rgb(128, 144, 0)
        "terminal.ansiWhite": "#C8D0C0", //rgb(200, 208, 192)
        "terminal.ansiBrightBlack": "#809070", //rgb(128, 144, 112)
        "terminal.ansiBrightBlue": "#2040E0", //rgb(32, 64, 224)
        "terminal.ansiBrightCyan": "#20E0D0", //rgb(32, 224, 208)
        "terminal.ansiBrightGreen": "#20E010", //rgb(32, 224, 16)
        "terminal.ansiBrightMagenta": "#E040E0", //rgb(224, 64, 224)
        "terminal.ansiBrightRed": "#E04800", //rgb(224, 72, 0)
        "terminal.ansiBrightYellow": "#D0E000", //rgb(208, 224, 0)
        "terminal.ansiBrightWhite": "#D8E8E0" //rgb(216, 232, 224)
    },
    "tokenColors": [{
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "meta.diff.header",
            "settings": {
                "foreground": "#100800" //rgb(16, 8, 0)
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#948686" //rgb(170, 153, 153)
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#53628d" //rgb(97, 113, 161)
            }
        },
        {
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#6d345f" //rgb(161, 97, 145)
            }
        },
        {
            "scope": "constant.regexp",
            "settings": {
                "foreground": "#94431b" //rgb(161, 97, 65)
            }
        },
        {
            "name": "css tags in selectors, xml tags",
            "scope": "entity.name.tag",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "entity.name.selector",
            "settings": {
                "foreground": "#100800" //rgb(16, 8, 0)
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#100800" //rgb(16, 8, 0)
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.class.css",
                "entity.other.attribute-name.class.mixin.css",
                "entity.other.attribute-name.parent-selector.css",
                "entity.other.attribute-name.pseudo-class.css",
                "entity.other.attribute-name.pseudo-element.css",
                "entity.other.attribute-name.attribute.scss",
                "entity.other.attribute-name.scss"
            ],
            "settings": {
                "foreground": "#324375", //rgb(97, 113, 161)
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.id.css",
                "source.css.less entity.other.attribute-name.id"
            ],
            "settings": {
                "foreground": "#6d345f" //rgb(161, 97, 145)
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#100800" //rgb(16, 8, 0)
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "markup.heading",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#53628d" //rgb(97, 113, 161)
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.list",
            "settings": {
                "foreground": "#6d345f" //rgb(161, 97, 145)
            }
        },
        {
            "scope": "markup.list.numbered",
            "settings": {
                "foreground": "#6d345f" //rgb(161, 97, 145)
            }
        },
        {
            "scope": "markup.list.numbered",
            "settings": {
                "foreground": "#6d345f" //rgb(161, 97, 145)
            }
        },
        {
            "scope": "markup.quote",
            "settings": {
                "foreground": "#347454" //rgb(81, 145, 113)
            }
        },
        {
            "scope": "markup.raw",
            "settings": {
                "foreground": "#347454" //rgb(81, 145, 113)
            }
        },
        {
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#347454" //rgb(81, 145, 113)
            }
        },
        {
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#00D080" //rgb(0, 208, 128)
            }
        },
        {
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#D00000" //rgb(208, 0, 0)
            }
        },
        {
            "scope": "markup.changed",
            "settings": {
                "foreground": "#0080F0" //rgb(0, 128, 240)
            }
        },
        {
            "scope": [
                "beginning.punctuation.definition.quote.markdown",
                "beginning.punctuation.definition.list.markdown"
            ],
            "settings": {
                "foreground": "#100800" //rgb(16, 8, 0)
            }
        },
        {
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#100800" //rgb(16, 8, 0)
            }
        },
        {
            "scope": "meta.selector",
            "settings": {
                "foreground": "#100800" //rgb(16, 8, 0)
            }
        },
        {
            "name": "brackets of XML/HTML tags",
            "scope": "punctuation.definition.tag",
            "settings": {
                "foreground": "#706050" //rgb(112, 96, 80)
            }
        },
        {
            "scope": "meta.preprocessor",
            "settings": {
                "foreground": "#100800" //rgb(16, 8, 0)
            }
        },
        {
            "scope": "meta.preprocessor.string",
            "settings": {
                "foreground": "#862121" //rgb(161, 49, 49)
            }
        },
        {
            "scope": "meta.preprocessor.numeric",
            "settings": {
                "foreground": "#6d345f" //rgb(161, 97, 145)
            }
        },
        {
            "scope": "meta.structure.dictionary.key.python",
            "settings": {
                "foreground": "#100800" //rgb(16, 8, 0)
            }
        },
        {
            "scope": "storage",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "storage.type",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "storage.modifier",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#862121" //rgb(161, 49, 49)
            }
        },
        {
            "scope": [
                "string.comment.buffered.block.jade",
                "string.quoted.jade",
                "string.interpolated.jade",
                "string.unquoted.plain.in.yaml",
                "string.unquoted.plain.out.yaml",
                "string.unquoted.block.yaml",
                "string.quoted.single.yaml",
                "string.quoted.double.xml",
                "string.quoted.single.xml",
                "string.unquoted.cdata.xml",
                "string.quoted.double.html",
                "string.quoted.single.html",
                "string.unquoted.html",
                "string.quoted.single.handlebars",
                "string.quoted.double.handlebars"
            ],
            "settings": {
                "foreground": "#862121" //rgb(161, 49, 49)
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#94431b" //rgb(161, 97, 65)
            }
        },
        {
            "name": "JavaScript string interpolation ${}",
            "scope": [
                "punctuation.definition.template-expression.begin.js",
                "punctuation.definition.template-expression.begin.ts",
                "punctuation.definition.template-expression.end.ts",
                "punctuation.definition.template-expression.end.js"
            ],
            "settings": {
                "foreground": "#94431b" //rgb(161, 97, 65)
            }
        },
        {
            "scope": [
                "support.constant.property-value",
                "support.constant.font-name",
                "support.constant.media-type",
                "support.constant.media",
                "constant.other.color.rgb-value",
                "constant.other.rgb-value",
                "support.constant.color"
            ],
            "settings": {}
        },
        {
            "scope": [
                "support.type.vendored.property-name",
                "support.type.property-name",
                "variable.css",
                "variable.scss",
                "variable.other.less"
            ],
            "settings": {}
        },
        {
            "scope": "support.type.property-name.json",
            "settings": {
                "foreground": "#100800" //rgb(16, 8, 0)
            }
        },
        {
            "scope": [
                "punctuation.section.embedded.begin.php",
                "punctuation.section.embedded.end.php"
            ],
            "settings": {}
        },
        {
            "scope": "support.function.git-rebase",
            "settings": {
                "foreground": "#0080F0" //rgb(0, 128, 240)
            }
        },
        {
            "scope": "constant.sha.git-rebase",
            "settings": {
                "foreground": "#00C080" //rgb(0, 192, 128)
            }
        },
        {
            "name": "coloring of the Java import and package identifiers",
            "scope": [
                "storage.modifier.import.java",
                "variable.language.wildcard.java",
                "storage.modifier.package.java"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "this.self",
            "scope": "variable.language",
            "settings": {
                "foreground": "#347454" //rgb(81, 145, 113)
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "keyword.control"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                //"foreground": "#347454"
            }
        },
        {
            "scope": [
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.cast",
                "keyword.operator.sizeof",
                "keyword.operator.logical.python"
            ],
            "settings": {
                "foreground": "#347454" //rgb(81, 145, 113)
            }
        },
        {
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#347454" //rgb(81, 145, 113)
            }
        },
        {
            "name": "Function declarations",
            "scope": [
                "entity.name.function",
                "support.function",
                "support.constant.handlebars"
            ],
            "settings": {
                "foreground": "#53628d" //rgb(97, 113, 161)
            }
        },
        {
            "name": "Types declaration and references",
            "scope": [
                "meta.return-type",
                "support.class",
                "support.type",
                "entity.name.type",
                "entity.name.class",
                "storage.type.cs",
                "storage.type.generic.cs",
                "storage.type.modifier.cs",
                "storage.type.variable.cs",
                "storage.type.annotation.java",
                "storage.type.generic.java",
                "storage.type.java",
                "storage.type.object.array.java",
                "storage.type.primitive.array.java",
                "storage.type.primitive.java",
                "storage.type.token.java",
                "storage.type.groovy",
                "storage.type.annotation.groovy",
                "storage.type.parameters.groovy",
                "storage.type.generic.groovy",
                "storage.type.object.array.groovy",
                "storage.type.primitive.array.groovy",
                "storage.type.primitive.groovy"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Types declaration and references, TS grammar specific",
            "scope": [
                "meta.type.cast.expr",
                "meta.type.new.expr",
                "support.constant.math",
                "support.constant.dom",
                "support.constant.json"
            ],
            "settings": {
                "foreground": "#6d345f" //rgb(161, 97, 145)
            }
        },
        {
            "name": "Inherited class, TS grammar specific",
            "scope": [
                "entity.other.inherited-class"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variable and parameter name",
            "scope": [
                "variable",
                "meta.definition.variable.name",
                "support.variable"
            ],
            "settings": {}
        },
        {
            "name": "Object keys, TS grammar specific",
            "scope": [
                "meta.object-literal.key",
                "meta.object-literal.key entity.name.function"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS property value",
            "scope": [
                "support.constant.property-value",
                "support.constant.font-name",
                "support.constant.media-type",
                "support.constant.media",
                "constant.other.color.rgb-value",
                "constant.other.rgb-value",
                "support.constant.color"
            ],
            "settings": {
                "foreground": "#53628d" //rgb(97, 113, 161)
            }
        }
    ]
}